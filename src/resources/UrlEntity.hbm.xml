<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.blacklighting.entity.UrlEntity" table="url" schema="spider">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="url">
            <column name="url" sql-type="varchar(256)" length="256" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="needHandJs">
            <column name="need_hand_JS" sql-type="tinyint(1)" not-null="false"/>
        </property>
        <property name="pageId">
            <column name="page_id" sql-type="int(10) unsigned" not-null="false"/>
        </property>
        <property name="weight">
            <column name="weight" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="retryTime">
            <column name="retry_time" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="deepth">
            <column name="deepth" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="domain">
            <column name="domain" sql-type="varchar(256)" length="256" not-null="false"/>
        </property>
        <property name="isSeed">
            <column name="is_seed" sql-type="tinyint(1)" not-null="false"/>
        </property>
        <property name="maxDeepth">
            <column name="max_deepth" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="md5">
            <column name="md5" sql-type="varchar(64)" length="64" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>