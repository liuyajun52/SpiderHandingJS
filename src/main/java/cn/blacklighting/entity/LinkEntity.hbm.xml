<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.blacklighting.entity.LinkEntity" table="link" schema="spider">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="fromUrl">
            <column name="from_url" sql-type="varchar(512)" length="512"/>
        </property>
        <property name="toUrl">
            <column name="to_url" sql-type="varchar(512)" length="512"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="updateTime">
            <column name="update_time" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="fromId">
            <column name="from_id" sql-type="int(11)"/>
        </property>
        <property name="toId">
            <column name="to_id" sql-type="int(11)"/>
        </property>
        <property name="text">
            <column name="text" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>