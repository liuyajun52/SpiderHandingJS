<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.blacklighting.entity.PageEntity" table="page" schema="spider">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="urlId">
            <column name="url_id" sql-type="int(10) unsigned" not-null="true"/>
        </property>
        <property name="pagePath">
            <column name="page_path" sql-type="varchar(256)" length="256" not-null="true"/>
        </property>
        <property name="docType">
            <column name="doc_type" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="updateTime">
            <column name="update_time" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="jsHandled">
            <column name="JS_handled" sql-type="tinyint(1)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>